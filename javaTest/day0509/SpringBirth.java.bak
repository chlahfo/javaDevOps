/*
	사용자에게 출생월 입력 받아 봄에 태어났느지 판별하여 결과를 출력
*/
import java.util.Scanner;
class SpringBirth
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int m;
		System.out.print("몇 월에 태어났습니까? ");
		m = sc.nextInt();

		if(m >= 3 && m <= 5 ){
			System.out.println("봄에 태어나셨습니다");
		}
	}
}
