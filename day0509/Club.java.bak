/*
	이름, 나이, 키를 입력받아 나이가 30살 이상이고 키가 170이하이면 "입장가능 그렇지 않으면 입장불가능을 출력하도록 합니다.
*/
import java.util.Scanner;
class Club
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		String name;
		int age;
		double h;
		System.out.print("이름: ");
		name = sc.next();
		System.out.print("나이: ");
		age = sc.nextInt();
		System.out.print("키: ");
		h = sc.nextDouble();
		if(age >= 30 && h <=170){
			System.out.println("입장가능");
		}else{
			System.out.println("입장불가능");
		}
	}
}
